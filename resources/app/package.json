{
  "name": "rolauncher",
  "version": "1.0.0",
  "description": "A simple launcher that lets you launch roblox applications quickly et easily",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win32":"electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=Assets/Imgs/rolauncher_icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE",
    "package-win64":"electron-packager . --overwrite --platform=win32 --arch=x64 --icon=Assets/Imgs/rolauncher_icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE"
  },
  "keywords": [
    "Roblox"
  ],
  "author": "Swardoric#9725",
  "license": "ISC",
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "discord-rich-presence": "0.0.8",
    "discord-rpc": "^3.1.0",
    "electron-updater": "^4.2.0",
    "update-electron-app": "^1.5.0"
  }
}
